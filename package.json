{
  "name": "diagnosticpro-legendary",
  "version": "5.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "playwright test",
    "test:headed": "HEADED=true playwright test",
    "test:all": "node tests/run-all-tests.js",
    "test:api": "playwright test api-endpoints.test.js",
    "test:ai": "playwright test ai-quality-validation.test.js",
    "test:email": "playwright test email-verification.test.js",
    "test:e2e": "playwright test e2e-complete-workflow.test.js",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:check",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx,.svelte --max-warnings 0",
    "lint:css": "stylelint \"**/*.{css,scss,svelte}\" --max-warnings 0",
    "lint:check": "npm run check",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint && npm run format:check && npm run test",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npm run security:audit",
    "analyze": "vite build --mode analyze",
    "clean": "rm -rf build .svelte-kit node_modules/.vite",
    "ci": "npm run lint && npm run format:check && npm run check && npm run build && npm run test",
    
    "deploy:dev": "gcloud builds submit --config cloudbuild.yaml --substitutions=_ENV=development",
    "deploy:staging": "gcloud builds submit --config cloudbuild.yaml --substitutions=_ENV=staging", 
    "deploy:prod": "gcloud builds submit --config cloudbuild.yaml --substitutions=_ENV=production",
    
    "cost:check": "gcloud billing budgets list --billing-account=$(gcloud billing projects describe $PROJECT_ID --format='value(billingAccountName)' | sed 's/.*\\///')",
    "cost:monitor": "gcloud logging read 'resource.type=\"cloud_run_revision\"' --limit=50 --format='table(timestamp,severity,textPayload)'"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.7.0",
    "@google/generative-ai": "^0.24.1",
    "@stripe/stripe-js": "^4.10.0",
    "@sveltejs/adapter-node": "^5.2.0",
    "@sveltejs/kit": "^2.5.0",
    "googleapis": "^144.0.0",
    "lucide-svelte": "^0.445.0",
    "openai": "^4.67.3",
    "pg": "^8.16.3",
    "stripe": "^18.4.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.54.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "playwright": "^1.54.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
